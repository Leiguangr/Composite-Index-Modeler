                                                if len(my_formula.children)>1: self.show_save("Files: my_index.csv, my_index.log")
                                                else: root.popuper("Empty formula..", "Warning!")

                                        Calc_Func_Btn:
                                            text: '+'
                                            on_press: root.calc_btn_pressed(self.text)

                            GridLayout:
                                cols: 2
                                spacing: (0, 4)

                                BoxLayout:
                                    size_hint_x: None
                                    width: 3

                                Label:
                                    size_hint_y: None
                                    height: 20
                                    font_size: 16
                                    bold: True
                                    text_size: (self.width-10, self.height-5)
                                    halign: "left"
                                    valign: "bottom"
                                    text: "Hint.."
                                    color: (0.3, 0.3, 0.3, 1)

                                FormulaHintItem:
                                FormulaHintLabel:
                                    text: '[color=f54236][b]Iterations:[/b][/color] [color=666666]This formula will be calculated for each selected Region and Year in "Series Selection". (Num of Iterations = Regions x Years)[/color]'
                                    height: 65

                                FormulaHintItem:
                                FormulaHintLabel:
                                    text: "[color=f54236][b]Functions:[/b][/color] [color=666666]All functions must return a number. If a function takes more than one parameters, use comma to separate them.[/color]"
                                    height: 65

                                FormulaHintItem:
                                FormulaHintLabel:
                                    text: '[color=f54236][b]Saving:[/b][/color] [color=666666]You must first select the location where results (my_index.csv) and log (my_index.log) will be stored.[/color]'
                                    height: 50

                                FormulaHintItem:
                                FormulaHintLabel:
                                    text: '[color=f54236][b]Validation:[/b][/color] [color=666666]Check log file for any syntax errors, or missing indicator values.[/color]'
                                    height: 50


