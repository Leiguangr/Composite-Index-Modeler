<MainWindow>:
    core_build_progress_bar: core_build_progress_bar
    coredb_state: coredb_state
    index_selection: index_selection
    coredb_update_popup: coredb_update_popup.__self__

    orientation: "horizontal"

    CIMScreenManager:
        transition: sm.SwapTransition()

        id: manager
        Screen:
            name: 'Home'

            BoxLayout:
                padding: 10,0,0,0
                canvas.before:
                    Color:
                        rgba: (0.2, 0.2, 0.2, 1)
                    Rectangle:
                        pos: self.pos
                        size: self.size

                ScrollView:
                    do_scroll_x: False
                    bar_width: 5
                    bar_inactive_color: (1, 1, 1, 1)
                    bar_color: (0.34, 0.76, 1, 1)
                    bar_margin: 1
                    bar_pos_y: "right"
                    canvas.before:
                        Color:
                            rgba: (1, 1, 1, 1)
                        Rectangle:
                            pos: self.pos
                            size: self.size

                    LabelTip:
                        size_hint: (None, None)
                        size: (self.texture_size[0]+40, self.texture_size[1]+40)
                        font_size: 15
                        markup: True
                        color: (0,0,0,1)

        IndexSelection:
            id: index_selection
            is_update_db: update_db
            is_manager: manager
            is_index_creation: index_creation
            ind_select_icon: ind_select_icon

        IndexCreation:
            id: index_creation
            ic_index_selection: index_selection
            ic_thematic_btn: thematic_btn
            ic_thematic_btn_box: thematic_btn_box
            ic_th_designer: th_designer

        MapDesigner:
            id: th_designer
            md_index_creation: index_creation

    BoxLayout:
        id: main_boxlayout
        orientation: "vertical"
        size_hint_x: None
        width: 100
        canvas.before:
            Color:
                rgba: (0.18, 0.18, 0.18, 1)
            Rectangle:
                pos: self.pos
                size: self.size


        Popup:
            id: coredb_update_popup
            size_hint: (None, None)
            height: 250
            width: 400
            title: "Update Indicator Database"
            on_parent: if self.parent == main_boxlayout: self.parent.remove_widget(self)

            BoxLayout:
                orientation: "vertical"

                Label:
                    id: coredb_state
                    size_hint_y: None
                    height: 80
                    text: "..."
                    text_size: (self.width, self.height)
                    halign: 'center'
                    valign: 'bottom'

                AnchorLayout:
                    id: core_build_progress_bar

                BoxLayout:
                    orientation: "horizontal"

                    Button:
                        disabled: root.processing
                        text: "Update"
                        on_release: root.threadonator(root.core_build)

                    Button:
                        text: "Return"
                        on_release:
                            coredb_update_popup.dismiss()
                            root.popup_active = False

        Label:
            size_hint: None, None
            size: 100, 30
            text: "MENU"
            font_size: 15
            text_size: (self.width, self.height)
            halign: 'center'
            valign: "bottom"
            bold: True

        AnchorLayout:
            anchor_x: "center"
            anchor_y: "top"

            BoxLayout:
                size_hint_y: None
                height: 370
                padding: 0, 5, 0, 5
                spacing: 40
                orientation: "vertical"

                BoxLayout:
                    size_hint_y: None
                    height:90
                    orientation: "vertical"

                    ToggleButton:
                        id: ind_select_icon
                        group: "menu"
                        size_hint: None, None
                        size: 100, 70
                        on_release:
